# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Account @key(fields: "id"){
    id: ID!
    name: String!
    email: String!
}

extend type Review @key(fields: "id"){
    id: ID! @external
    author: Account @requires(fields: "id")
}

type Query {
    accounts(count: Int = 10): [Account!]!
}